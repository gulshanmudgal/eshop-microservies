  "version": "0.2.0",
  "configurations": [
    {
      "name": "Catalog API [https]-Debug",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/src/Services/Catalog/Catalog.API/bin/Debug/net8.0/Catalog.API.dll",
      "cwd": "${workspaceFolder}/src/Services/Catalog/Catalog.API",
      "launchSettingsProfile": "https",
      "args": [],
      "stopAtEntry": false,
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "internalConsoleOptions": "openOnSessionStart"
    },
    {
      "name": "Catalog API [http]-Debug",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/src/Services/Catalog/Catalog.API/bin/Debug/net8.0/Catalog.API.dll",
      "cwd": "${workspaceFolder}/src/Services/Catalog/Catalog.API",
      "launchSettingsProfile": "http",
      "args": [],
      "stopAtEntry": false,
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "internalConsoleOptions": "openOnSessionStart"
    }
  ]
}
